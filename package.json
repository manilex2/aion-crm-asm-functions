{
  "name": "aion-crm-functions",
  "version": "1.0.0",
  "description": "Funciones Cloud de Firebase para la App de AION CRM",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "serve": "NODE_ENV=dev firebase emulators:start --only functions",
    "shell": "NODE_ENV=dev firebase functions:shell",
    "start": "npm run shell",
    "deploy": "NODE_ENV=production firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "private": true,
  "dependencies": {
    "firebase-functions": "^5.1.1"
  }
}
